<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mixi.assignment.serializeandcollection">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SerializableListActivity"
            android:label="@string/serializable_name" />
        <activity
            android:name=".SerializableDetailActivity"
            android:label="@string/serializable_detail_name" />
        <activity
            android:name=".SharedPreferencesActivity"
            android:label="@string/shared_preferences_name" />
    </application>
</manifest>
